  },
  "homepage": "https://github.com/FranciscoHeronildo/heartbeat#readme",
  "dependencies": {
    "axios": "^1.5.1",
    "child_process": "^1.0.2",
    "dotenv": "^16.3.1",
    "eslint": "^8.51.0",
    "express": "^4.18.2",
    "fs": "^0.0.1-security",
    "nodemon": "^3.0.1",
    "pkg": "^5.8.1",
    "typescript": "^5.2.2"
